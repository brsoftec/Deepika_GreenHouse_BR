<!DOCTYPE html>
<html ng-app="regitPublic" ng-cloak>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Regit Interaction">
    <meta property="og:title" content="Regit Interaction" />
    <link href="//fonts.googleapis.com/css?family=Roboto:300,400,500,700" rel="stylesheet">
    <title>Regit - @ViewBag.Title</title>
    @Styles.Render("~/bundles/css/main")

    @Styles.Render("~/bundles/css/public")


    <link href="~/Areas/Beta/css/public-interaction.css" rel="stylesheet">

    @RenderSection("styles", required: false)
    <link rel="stylesheet" href="~/Areas/Beta/css/responsive.css">
</head>
<body class="regit-public regit-public-interaction">
    @Html.Partial("_PublicHeaderPartial")
    <div class="main-body">
        @RenderBody()
    </div>
    @Html.Partial("_PublicFooterPartial")

    @Scripts.Render("~/bundles/js/vendor")

    @Scripts.Render("~/bundles/js/main")
    @Scripts.Render("~/bundles/js/greenhouse")
    @Scripts.Render("~/bundles/js/greenhouseBusiness")
    @Scripts.Render("~/bundles/user-service")
    @Scripts.Render("~/bundles/js/phone-input")
    @Scripts.Render("~/bundles/js/public")

    <script src="~/Areas/Beta/js/modules/campaigns.js"></script>

    <script src="~/Scripts/regit-responsive.js"></script>

    @RenderSection("scripts", required: false)


</body>
</html>
