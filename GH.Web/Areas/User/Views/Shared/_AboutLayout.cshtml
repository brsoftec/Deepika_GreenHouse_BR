@{
    ViewBag.PageKeywords = "regit, social, identity, personal, share, organize, network, web, mobile ";
ViewBag.About = true;
    //ViewBag.NgApp = "regitPublic";
}


<!DOCTYPE html>
<html ng-app="@ViewBag.NgApp" ng-cloak>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="@ViewBag.Title">
    <meta name="keywords" content="@ViewBag.PageKeywords" />
    <meta property="og:url" content="https://regit.today">
    <meta property="og:type" content="website" />
    <meta property="og:site_name" content="Regit" />
    <meta property="og:title" content="Regit" />
    <meta property="og:description" content="@ViewBag.PageDescription" />
    <meta property="og:image" content="https://regit.today/Areas/About/Content/img/regit-logo-s.png" />
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:title" content="Regit">
    <meta name="twitter:description" content="@ViewBag.Title">
    <meta name="twitter:creator" content="QUDY">
    <meta name="twitter:image" content="https://regit.today/Areas/About/Content/img/regit-logo-s.png">
    <meta name="twitter:url" content="https://regit.today">
    <meta name="twitter:domain" content="regit.today">

    <link href="//fonts.googleapis.com/css?family=Roboto:300,400,500,700" rel="stylesheet">

    <title>@ViewBag.Title</title>


    @*@Styles.Render("~/bundles/css/main")*@

    <link rel="stylesheet" href="~/Areas/Beta/css/regit-main.css">

    @Styles.Render("~/bundles/css/about")


    @RenderSection("styles", required: false)

                <link rel="stylesheet" href="~/Areas/About/Content/css/legal.css">

        <link rel="stylesheet" href="~/Areas/Beta/css/responsive.css">

    <script src="~/Areas/About/Content/js/modernizr-custom.js"></script>
    
        
    @RenderSection("prescripts", required: false)


</head>
<body class="home @ViewBag.BodyClass" resize>
    @Html.Partial("_PublicHeaderPartial")

        @RenderBody()

    @Html.Partial("_AboutFooterPartial")

   
    @Scripts.Render("~/bundles/js/public-vendor")

    @Scripts.Render("~/bundles/sweet-alert")
    @Scripts.Render("~/bundles/js/phone-input")



@RenderSection("initscripts", required: false)


<script src="~/Areas/Beta/js/greenhouseApp.js"></script>
    @Scripts.Render("~/bundles/js/greenhouse")


    @Scripts.Render("~/bundles/js/public")
    @Scripts.Render("~/bundles/js/about")

    <script src="~/Scripts/angular-sanitize.min.js"></script>

    <script src="~/Areas/regitUI/js/modules/campaigns.js"></script>
    <script src="~/Areas/regitUI/js/modules/defs.js"></script>
    <script src="~/Content/scripts/international-phone-number.min.js"></script>
    <script src="~/Areas/regitUI/js/modules/regit.ui.js"></script>
    <script src="~/Areas/Beta/js/vendor/angular-strap.min.js"></script>
    <script src="~/Areas/Beta/js/vendor/angular-momentjs.js"></script>
@*<script src="~/Scripts/regit-responsive.js"></script>*@

@RenderSection("scripts", required: false)

    @Html.Partial("_GoogleAnalyticsScript")

</body>
</html>
